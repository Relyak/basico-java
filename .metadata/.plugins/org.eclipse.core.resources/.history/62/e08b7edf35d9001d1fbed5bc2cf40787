package ejercicioArrayNumeros;

import static org.junit.jupiter.api.Assertions.*;

import org.junit.jupiter.api.Test;

class AppTest {
	App app=new App();
	int numEsperado;
	int [] num= {0,-1,7};
	@Test
	void testSuma() {
		numEsperado = 15;
		assertEquals(numEsperado, app.suma().apply(null));
	}

}
