package ejemploGenerico;

public class App {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Generica <Integer>prueba =new Generica<Integer>();
		prueba.setPrimera(2);
	}

}
class Generica <T>{
	private T primera;
	
	
	public Generica() {
		this.primera=null;
	}
	
	public T getPrimera() {
		return this.primera;
	}
	public void setPrimera(T valor) {
		this.primera=valor;
	}
	
	
	
	
}
